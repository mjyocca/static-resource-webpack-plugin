{
    "name": "static-resource-webpack-plugin",
    "version": "1.0.0",
    "description": "webpack plugin that reads the static resource directory of your sfdx project and creates the needed xml files required to deploy to salesforce",
    "author": {
        "name": "Michael Yocca @mjyocca"
    },
    "main": "build/index.js",
    "scripts": {
        "build": "babel src -d build"
    },
    "repository": "https://github.com/mjyocca/static-resource-webpack-plugin.git",
    "bugs": "https://github.com/mjyocca/static-resource-webpack-plugin/issues",
    "keywords": [
        "staticresource",
        "salesforce",
        "webpack",
        "plugin"
    ],
    "files": [
        "build",
        "schema.json"
    ],
    "license": "MIT",
    "dependencies": {
        "schema-utils": "^2.6.4",
        "xmlbuilder": "^13.0.2"
    },
    "peerDependencies": {
        "webpack": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "babel-polyfill": "^6.26.0"
    }
}
